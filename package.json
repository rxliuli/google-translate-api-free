    "syncpack": "^5.6.10",
    "ts-jest": "^26.5.4",
    "type-fest": "^1.0.1"
  },
  "workspaces": {
    "packages": [
      "libs/*",
      "examples/*"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,css,md,json}": [
